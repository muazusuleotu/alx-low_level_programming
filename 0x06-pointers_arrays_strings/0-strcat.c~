#include "main.h"

/**
*
*
*
*/

char *_strcat(char *src2, char *src)
{
  int count = 0, count2 = 0;

  while (*(src2 + count) != '\0')
    {
      
      count++;
      
    }
  
  while (count2 >= 0)
    
    {
      
      *(src2 + count) = *(src + count2);
      
      if (*(src + count2) == '\0')
	
	break;
      
      count++;
      
      count2++;
      
    }
  
  return (src2);
  
}

